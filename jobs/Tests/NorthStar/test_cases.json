		"status": "active",
		"functions": [
			"material = [e for e in bpy.data.materials if e.name == 'Default'][0]",
			"uber = [n for n in material.node_tree.nodes if n.name=='Material Output'][0]",
			"tree = material.node_tree",
			"node_uber = tree.nodes.new(type='RPRShaderNodeUber')",
			"node_uber.inputs['Diffuse Color']['default_value'] = (0, 0.4, 0, 1.0)",
			"tree.links.new(node_uber.outputs['Shader'], uber.inputs['Surface'])",
			"rpr_render(case)",
			"bpy.ops.wm.open_mainfile(filepath=os.path.join(RES_PATH, 'default.blend'))"
